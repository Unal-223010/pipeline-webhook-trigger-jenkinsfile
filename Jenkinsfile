pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo "Clarusway_Way to Reinvent Yourself"
                sh 'echo using shell within Jenkinsfile'
                echo 'not using shell in the Jenkinsfile'
            }
        stage('deneme') {
            steps {
                echo "deneme"
                sh 'change the jenkinsfile'
                echo 'not using shell in the Jenkinsfile'
            }
        }
    }
}
